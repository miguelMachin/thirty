<button class="w3-button w3-padding-large" title="Notificaciones"><i class="fa fa-bell"></i>     
{{#if noRead}}
    <span class="w3-badge w3-right w3-small w3-green">{{noRead}}</span></button>
{{else}}
    <span class="w3-badge w3-right w3-small w3-green"></span></button>
{{/if}}
     <div id="not" class="w3-dropdown-content w3-card-4 w3-bar-block" style="width:380px">
        {{#each not}}
            <div class="notification" onclick="readNotification('{{{this.not._id}}}');" onmouseover="show('{{{this.not._id}}}');" onmouseout="hide('{{{this.not._id}}}');">
                <span id="{{this.not._id}}" class="closeNotification hidden color607d8b2" id="deleteNotification" onclick="deleteNotification('{{{this.not._id}}}')"><i class="fa fa-times" aria-hidden="true"></i>
                </span>
                {{#if this.not.isFriend}}
                    <span class="marginBottom10px">{{{this.name}}} ahora es tu amigo</span>
                {{else}}
                    <span class="marginBottom10px">{{{this.name}}} le ha gustado tu publicaci√≥n</span>
                {{/if}}
             </div>
        {{/each}}
    </div>
